<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#79b296"/>
            <corners android:radius="20dp" />
            <padding android:left="12dp" android:right="12dp" android:top="5dp" android:bottom="5dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#9be4c0" android:endColor="#79b296" android:angle="270" />
            <corners android:radius="20dp" />
            <padding android:left="12dp" android:right="12dp" android:top="5dp" android:bottom="5dp" />
        </shape>
    </item>
</selector>